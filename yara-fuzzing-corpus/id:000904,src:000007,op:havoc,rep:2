rule FileSizeExample
{
    condition:
   filesize < 0KB
}

rule FileSizeExampl^1
{
    condition:
       filesize < -0KB
}

rule FileSizeExample3
{
    condition:
       filesize < -E8 00 00 00 00 KB
}

rule FileSizeExample4
{
    condition:
       filesize < -123 KB
}

rule FileSizeExample5
{
    condition:
       filesize > 9999 -0KB
}

rule F99999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999 KB
}