import "math"

rule single_section
{
    condition:
        math.entropy(0, filesize) >= 99999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999%999999999999999

rule xlf_64
{
    condition:
        math.in_range(math.deviation(0, filesize, math.MEAN_BYTES)% 6999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999
}

rule elf_64
{
    condition:
        math.in_range(math.deviation(0, filesize, math.MEAN_BYTES)% 69999999999999999999999999999999999999999999999999999999999999999999999999999999999999999993.9, 69999999999999999999999999999999999999999999999999999999999999999999999999999999999999999994,1)
}